{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df2408"
		},
		"datalake2496_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake2496'"
		},
		"datalake2496_activity_csv_log_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake2496_activity_csv_log'"
		},
		"synapseworkspace240896_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'synapseworkspace240896'"
		},
		"datalake2496_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake2496.dfs.core.windows.net/"
		},
		"datalake2496_activity_csv_log_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake2496.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df2408_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalake2496_accountKey": {
						"value": "[parameters('datalake2496_accountKey')]"
					},
					"datalake2496_activity_csv_log_accountKey": {
						"value": "[parameters('datalake2496_activity_csv_log_accountKey')]"
					},
					"synapseworkspace240896_connectionString": {
						"value": "[parameters('synapseworkspace240896_connectionString')]"
					},
					"datalake2496_properties_typeProperties_url": {
						"value": "[parameters('datalake2496_properties_typeProperties_url')]"
					},
					"datalake2496_activity_csv_log_properties_typeProperties_url": {
						"value": "[parameters('datalake2496_activity_csv_log_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}